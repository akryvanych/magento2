<?xml version="1.0"?>
<config>
    <modules>
        <Tsg_EventExport>
            <version>0.0.7</version>
        </Tsg_EventExport>
    </modules>
    <global>
        <blocks>
            <tsg_eventexport>
                <class>Tsg_EventExport_Block</class>
            </tsg_eventexport>
            <promotions>
                <rewrite>
                    <adminhtml_catalogEvent_event_edit_tabs>Tsg_EventExport_Block_Adminhtml_CatalogEvent_Event_Edit_Tabs</adminhtml_catalogEvent_event_edit_tabs>
                </rewrite>
            </promotions>
        </blocks>
        <models>
            <tsg_eventexport>
                <class>Tsg_EventExport_Model</class>
                <resourceModel>tsg_eventexport_resource</resourceModel>
            </tsg_eventexport>
            <tsg_eventexport_resource>
                <class>Tsg_EventExport_Model_Resource</class>
                <entities>
                    <customEvent>
                        <table>tsg_eventexport_custom_event</table>
                    </customEvent>
                    <customEvent_products>
                        <table>tsg_eventexport_custom_event_products</table>
                    </customEvent_products>
                    <customEvent_log>
                        <table>tsg_eventexport_custom_event_log</table>
                    </customEvent_log>
                </entities>
            </tsg_eventexport_resource>
        </models>
        <resources>
            <tsg_eventexport_setup>
                <setup>
                    <module>Tsg_EventExport</module>
                </setup>
            </tsg_eventexport_setup>
        </resources>
        <helpers>
            <tsg_eventexport>
                <class>Tsg_EventExport_Helper</class>
            </tsg_eventexport>
        </helpers>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <tsg_eventexport before="Mage_Adminhtml">Tsg_EventExport_Adminhtml</tsg_eventexport>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <tsg_eventexport>
                    <file>tsg_eventexport.xml</file>
                </tsg_eventexport>
            </updates>
        </layout>
        <translate>
            <modules>
                <Tsg_EventExport>
                    <files>
                        <default>Tsg_EventExport.csv</default>
                    </files>
                </Tsg_EventExport>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Tsg_EventExport>
                    <files>
                        <default>Tsg_EventExport.csv</default>
                    </files>
                </Tsg_EventExport>
            </modules>
        </translate>
    </frontend>
    <default>
        <tsg_eventexport>
            <general>
                <log_enabled>1</log_enabled>
                <log_lifetime>5</log_lifetime>
            </general>
            <cron>
                <schedule backend_model="adminhtml/system_config_backend_serialized_array">{"enterprise_pci_locks":"1","adminhtml_system_account":"1","adminhtml_permission_roles":"1","adminhtml_permission_users":"1","admin_login":"1","cms_blocks":"1","enterprise_cms_hierarchy":"1","cms_pages":"1","poll":"1","enterprise_cms_page_versioning":"1","catalog_attributesets":"1","catalog_attributes":"1","catalog_categories":"1","tax_product_tax_classes":"1","catalog_products":"1","rating":"1","review":"1","tag":"1","adminhtml_system_variable":"1","enterprise_invitation":"1","enterprise_pci_crypt":"1","enterprise_giftregistry_entity":"1","enterprise_giftregistry_type":"1","googlebase_types":"1","adminhtml_system_convert_gui":"1","index_management":"1","adminhtml_system_design":"1","adminhtml_system_stores":"1","adminhtml_system_websites":"1","paypal_settlement_reports":"1","adminhtml_sales_recurring_profile":"1","enterprise_reward_rate":"1","enterprise_salesarchive":"1","sales_creditmemos":"1","sales_invoices":"1","sales_order_status":"1","sales_orders":"1","sales_shipments":"1","enterprise_checkout":"1","adminhtml_system_backups":"1","adminhtml_system_config":"1","tax_rates":"1","core_urlrewrites":"1","api_users":"1","api_roles":"1","widget_instance":"1","enterprise_catalogevent":"1","customer_groups":"1","google_sitemap":"1","tax_customer_tax_classes":"1","customer":"1","enterprise_customerbalance":"1","tax_rules":"1","reports":"1","newsletter_queue":"1","enterprise_giftcardaccount":"1","newsletter_subscribers":"1","catalogsearch":"1","sales_agreement":"1","enterprise_targetrule":"1","adminhtml_system_email_template":"1","adminhtml_customer_attribute":"1","adminhtml_customer_address_attribute":"1","enterprise_banner":"1","adminhtml_system_currency":"1","cache_management":"1","adminhtml_system_store_groups":"1","enterprise_customersegment":"1","catalogrule":"1","salesrule":"1","newsletter_templates":"1"}</schedule>
            </cron>
        </tsg_eventexport>
    </default>
</config>
